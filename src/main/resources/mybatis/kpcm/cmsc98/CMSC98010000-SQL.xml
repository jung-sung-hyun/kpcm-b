<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kp.cmsc.cmsc98.dao.Cmsc98010000Dao">
    <!-- 
       - SQL  명  : 게시판 조회 리스트
       - 작 성 자 : 정성현
       - 작 성 일 : 2024-05-13
       - 파라미터
         1. title    String(500)//시작일자
         2. contents String(4000)//종료일자
       - 이력관리
    =================수정일자/요청자/내용 =================
    1. 2024.04.12/ 정성현/ 최초작성
    =======================================================
    -->
    <select id="selectList00" parameterType="cmsc98010000VO" resultType="cmsc98010000VO">
        /*Cm9801Dao.selectCommonCodeList[게시판리스트조회]*/
         SELECT GRP_CD
         ,      CODE_CD
         ,      CODE_NM
           FROM TEST_CODE
          WHERE GRP_CD IN
          <foreach item="item" index="index" collection="list" open="(" separator="," close=")">
                #{item.codeCd}
          </foreach>
       ORDER BY GRP_CD
    </select>
</mapper>